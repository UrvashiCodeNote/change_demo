<h1><%= @customer.first_name %></h1>
<h1><%= @customer.last_name %></h1>
<h1><%= @customer.email %></h1>
<h1><%= @customer.aadhar_number %></h1>
<h1><%= @customer.aadhar_image %></h1>

<h1>Addresses:</h1>
<% @customer.addresses.each do |add| %>
  <p>Address: <%= add.address %></p>
<% end %>

<h1>Phone Numbers:</h1>
<% @customer.contact_books.each do |contact| %> 
  <p>Phone: <%= contact.phone %></p>
<% end %>
